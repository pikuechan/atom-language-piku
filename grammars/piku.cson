'name': 'Piku'
'scopeName': 'source.piku'
'fileTypes': [
  'txt',
  'piku'
]
'patterns': [
  {
    'match': '(^[▼▶].*)'
    'name': 'markup.heading.h2.piku'
  }
  {
    'match': '(^・.*)'
    'name': 'markup.heading.h3.piku'
  }
  {
    'match': '(^[※\*].*)'
    'name': 'entity.name.note.piku'
  }
  {
    'match': '([＜\\[].*?[\\]＞])'
    'name': 'markup.heading.h4.piku'
  }
  {
    'match': '([（\(].*?[\)）])'
    'name': 'comment.block.piku'
  }
  {
    'match': '(^#.*)'
    'name': 'comment.line.piku'
  }
  {
    'match': '([０-９0-9]+[\.．][^ 　]*)'
    'name': 'markup.list.numbered.bold.piku'
  }
  {
    'match': '([^\\s:：]*?:\/\/[^\\s]*)'
    'name': 'entity.uri.piku'
  }
  {
    'match': '([^\\s:：]*?@[^\\s]*)'
    'name': 'entity.email.piku'
  }
  {
    'match': '(^[^\\s]*?[:：])'
    'name': 'storage.type.piku'
  }
]
